+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (663861, 'Felix', '[{\"Customer\":\"Hey Felix\"},{\"Felix\":\"Hello! Welcome to the Talkative website. My name is Felix. How can I help you?\"},{\"Customer\":\"I am shopping around for a live chat service for the College i work for. We would like multiple live chat options for different webpages while also integrating with Salesforce\"},{\"Felix\":\"We can definitely help with that. You can customize the live chat behaviour/UI on a per page basis\"},{\"Felix\":\"We are on the AppExchange and have a \\\"native\\\" Salesforce integration\"},{\"Customer\":\"Awesome sounds good so far\"},{\"Felix\":\"If you\'d like to set up a demo, you can do so on our website. Or I can take your email now and send you some more info over\"},{\"Customer\":\"Regarding your pricing options, does 3 user limit to how many chat boxes we can have?\"},{\"Felix\":\"No, in theory you could have 1 user and 100 chat designs. From a pricing perspective, we have our licence cost (per user) but this doesn\'t limit the number of chat designs\"},{\"Felix\":\"How many chat boxes are you thinking of?\"},{\"Customer\":\"at least 7 different chat boxes\"},{\"Felix\":\"ok\"},{\"Customer\":\"So will the user just have to be logged into their Salesforce account to interact in the chat boxes?\"},{\"Felix\":\"The chat widget will show as \\\"online\\\" if 1. you\'re within office hours that are set in the platform, and 2. if you have agents online\"},{\"Felix\":\"when the widget is offline, you can hide the widget or show an email collector form\"},{\"Customer\":\"Okay cool\"},{\"Felix\":\"If you want more info feel free to reach out at <email>\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (672991, 'Felix', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Hi\"},{\"Talkative\":\"Hi! How are you doing?\"},{\"Customer\":\"Good thanks\"},{\"Talkative\":\"Sorry, I didn\'t quite catch that. Please rephrase your question, or click below to speak to the team:\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Felix\":\"Hello! Welcome to the Talkative website. My name is Felix. How can I help you?\"},{\"Customer\":\"HI\"},{\"Customer\":\"My name is Paul, I am Head of Sales for company-name.co.uk\"},{\"Customer\":\"We would like to take the Free Trial\"},{\"Customer\":\"During the Trial can i connect to Mitel Ignite?\"},{\"Felix\":\"Hi Paul, you sure can\"},{\"Customer\":\"perfect thank you.\"},{\"Felix\":\"Let me know your email and I\'ll set you up with an account now\"},{\"Customer\":\"<email>\"},{\"Felix\":\"Got it thanks. There is a bit of set up to do with your Mitel system to enable Talkative integration. I\'ll email through the set up guide, typically your Mitel partner would do it for you\"},{\"Customer\":\"Thank you, the guide will be useful, I will send this over to our IT team\"},{\"Felix\":\"Sending the email over now, thanks\"},{\"Customer\":\"Ok. I have not received the email yet, let me know when its sent.\"},{\"Felix\":\"sent\"},{\"Customer\":\"Thank you\"},{\"Customer\":\"I will speak with out IT and Web team and get the Trial going asap\"},{\"Customer\":\"thanks for your help today.\"},{\"Felix\":\"no problems, speak soon!\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (673143, 'Felix', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"hi, do you have a screenshot of how cobrowsing works?\"},{\"Talkative\":\"Sorry, I didn\'t quite catch that. Please rephrase your question, or click below to speak to the team:\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Felix\":\"Hi, what kind of screenshot are you looking for?\"},{\"Customer\":\"of how a customer see a cobrowsing session\"},{\"Felix\":\"would you like me to send through a picture, or we can cobrowse now?\"},{\"Customer\":\"we can cobrowse now\"},{\"Customer\":\"so i can see it\"},{\"Felix\":\"so now i can see your screen and annotate the page\"},{\"Customer\":\"ok, how exaclty i can see your cursor, for example\"},{\"Customer\":\"is a bit difficult to see the difference\"},{\"Felix\":\"are you looking to use cobrowse for your company?\"},{\"Customer\":\"yes\"},{\"Customer\":\"but i need to see how a customer will see it\"},{\"Felix\":\"ok\"},{\"Customer\":\"oh, ok i see it\"},{\"Felix\":\"so on my side i can choose to either draw, or show my mouse to you\"},{\"Felix\":\"or click on links\"},{\"Customer\":\"great, thanks\"},{\"Customer\":\"thats all what i need\"},{\"Customer\":\"have a good day\"},{\"Felix\":\"https://eu-engage-app.s3.eu-west-2.amazonaws.com/files/95cfe7a1-c72a-41b0-9d99-e005b6814287\"},{\"Felix\":\"ok thanks!\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (679458, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Hi There  - I am working on a project and looking at alternatives for webchat  and came across the video for talkative\"},{\"Talkative\":\"Sorry, I didn\'t quite catch that. Please rephrase your question, or click below to speak to the team:\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Customer\":\"we are using Mitel and looking webchats which integrate with the contact centre Ignite\"},{\"Steve\":\"Hello! Welcome to the Talkative website. My name is Steve. How can I help you?\"},{\"Steve\":\"Sorry for the delay, our Chatbot shouldn\'t have let you get through till we were available\"},{\"Steve\":\"Happy to discuss your requirement further here or you can contact me at swilliams@talkative.uk\"},{\"Customer\":\"hi Steve\"},{\"Customer\":\"are you still there\"},{\"Steve\":\"yes, again, sorry to keep you waiting. We aren\'t staffed for your timezone as you can probably tell.\"},{\"Steve\":\"Are you a customer or a Mitle partner?\"},{\"Steve\":\"Mitek\"},{\"Customer\":\"I am a contractor for a company that is using Mitel and Ignite\"},{\"Customer\":\"and we are looking at options for chat\"},{\"Customer\":\"and came across the talkative\"},{\"Customer\":\"but a few people are keen to use Zendesk\"},{\"Steve\":\"understand, yes we provide Chat, Webvoice, Webvideo and Cobrowse. Importantly delivered directly into Ignite so you can blend the website interactions with and Voice / Email you may be handling in Mitek today\"},{\"Steve\":\"Are you able to share the company or sector you are contracting for?\"},{\"Steve\":\"Happy to arrange a demonstration sometime where we can show you the seamless working with Mitel\"},{\"Customer\":\"Hospitality sector -\"},{\"Customer\":\"i might get a demo as it does look good  -but I need to do a summary of features and benefits\"},{\"Steve\":\"Ok, share me your email and I will send you over some info\"},{\"Customer\":\"should I take your details and follow up\"},{\"Customer\":\"<email>\"},{\"Customer\":\"thanks\"},{\"Steve\":\"Ok Melissa, I will send you an email in 5 minutes. Ding me back here if it doesn\'t get through. And then let me know when you want to follow up. We are UK based so timezones aren\'t ideal but I can accomodate a suitable time for you with a few days notice\"},{\"Customer\":\"ok cool\"},{\"Customer\":\"I noticed your comartive section on the website but noticed that each one is not always comparing like with like\"},{\"Customer\":\"e.g. data security is not listed on the LIveperson comparison but is on the Livechat\"},{\"Steve\":\"There\'s not an ideal comparison unfortunately however our integration with Mitel is unique and most other platform are only providing Voice, Video and Cobroese with plugins at additional cost. And usually not integrated well\"},{\"Customer\":\"good to know\"},{\"Steve\":\"I\'ll send info over but let me know of any questions or further info we can provide and endeavour to get that to you too\"},{\"Customer\":\"as you are in the UK - what is the support provided?  Would it be through the partner that we have in Melbourne?\"},{\"Customer\":\"thanks Steve\"},{\"Steve\":\"Yes for Mitel we typically use a local partner. But it\'s not essential, we can deliver our solution directly however there may be work on the Mitel needed\"},{\"Steve\":\"We use Open Media which requires at least 9.2 MiCC and relevant multimedia licences. Also you neeed to configure the Openmedia connection.\"},{\"Customer\":\"i am trying to scroll back to a part of the chat as you mentioned something earlier and I did not read the full note\"},{\"Customer\":\"is there a way I can go back to begining of chat\"},{\"Steve\":\"you should have scroll bars. Not sure why you can\'t access them\"},{\"Customer\":\"it does not seem to be letting me\"},{\"Customer\":\"yes I do have a scroll bar but it keeps jumping ahead\"},{\"Steve\":\"I can see you are using Chrome on a Windows 10 machine. That should be all fine\"},{\"Steve\":\"I\'ll take a look , 1 second\"},{\"Steve\":\"I\'m just sending in a test chat to myslef.\"},{\"Customer\":\"it is ok - i found it - it was just in regards to combining chat with the email and voice from Mitel\"},{\"Customer\":\"I am not sure they would want to change the ignite voice and email - they are just not super sold on the Ignite chat functionality so wanted to look at options\"},{\"Customer\":\"let me know when you send through the mail and I will let you know\"},{\"Steve\":\"Ok, yes Talkative can be used Standalone or embedded into Ignite. Is it Ignite they don\'t like? Or Mitel\'s chat tool? When you use Talkative we replace the Mitel chat with our own\"},{\"Customer\":\"Mitel chat tool\"},{\"Steve\":\"ok, so worth a demo to show the enhanced chat we provide, combined with cobrowse and option to escalate to realtime with Voice / Video\"},{\"Customer\":\"yes that is what I thought ,  We would look to keep the email and voice component and look to use the chat and other social media integrations\"},{\"Customer\":\"yes definitely need to arrange and demo\"},{\"Customer\":\"I think probably some time next week - will need to confirm  times\"},{\"Customer\":\"how long would a demo take?\"},{\"Steve\":\"usually 45 minutes\"},{\"Customer\":\"ok thanks\"},{\"Steve\":\"sending the email now\"},{\"Customer\":\"thanks a lot Steve\"},{\"Steve\":\"you\'re welcome, again, sorry for the delay. I\'ve got my best man working on it as you shouldn\'t have been able to get into queue without someone there to talk to you\"},{\"Steve\":\"Yes, the chatbot was new last week and should follow our business hours. We think we had a test agent logged in somewhere that overrode it all\"},{\"Customer\":\"i did wonder what was happening but then went off and did a few other things so not problem but it would be good to now how it happened.  I noticed that it say I was the next in queue but it does not mention time.  Is that an option to set up? just an estimate wait time?\"},{\"Steve\":\"We can do position in queue for Mitel integrations. Not time\"},{\"Customer\":\"ok - thanks\"},{\"Steve\":\"Also widget on website should only appear in business hours and when the max number of queueing callers is in queue. Something failed today\"},{\"Customer\":\"technology .... never 100% perfect :)\"},{\"Customer\":\"stay safe and well and I will be in touch soon\"},{\"Steve\":\"yes, not like me and you Melissa\"},{\"Customer\":\"lol\"},{\"Steve\":\"No problem ,look forward to meeting online sometime soon. Stay safe\"},{\"Customer\":\"you too - thanks a bunch for all your help today\"},{\"Steve\":\"no trouble. have a good day / evening\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (681386, 'Felix', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Support\"},{\"Talkative\":\"If you\'re have a technical issue, please email support@talkative.uk or transfer to the team. Please note we have a support portal here for FAQs: https://support.talkative.uk/\"},{\"Customer\":\"Sprechen Sie deutsch?\"},{\"Talkative\":\"Sorry, I didn\'t quite catch that. Please rephrase your question, or click below to speak to the team:\"},{\"Customer\":\"Bist du ein Bot?\"},{\"Talkative\":\"Sorry, I didn\'t quite catch that. Please rephrase your question, or click below to speak to the team:\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Customer\":\"Hallo\"},{\"Felix\":\"Hello! Welcome to the Talkative website. My name is Felix. How can I help you?\"},{\"Customer\":\"Sprechen Sie deutsch?\"},{\"Felix\":\"Yes\"},{\"Customer\":\"Sind Sie ei nBot?\"},{\"Felix\":\"No! My name is Felix\"},{\"Customer\":\"Auch Bots können Namen haben.\"},{\"Customer\":\"Fängt der Live Chat mit einem Bot an?\"},{\"Felix\":\"You can either start with a bot, or go straight through to a human agent queue\"},{\"Felix\":\"I am using real-time translation feature, apologies if my German isn\'t accurate!\"},{\"Customer\":\"Das ist leider schlecht. Vielen Dank.\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (682477, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Steve\":\"Hello! Welcome to the Talkative website. My name is Steve. How can I help you?\"},{\"Customer\":\"Hi Steve!!!\"},{\"Customer\":\"Thanks for your help\"},{\"Customer\":\"I want to try all your functionality integrated with my salesforce\"},{\"Customer\":\"Live chat, call, sharing, co browsing for our online journey experience\"},{\"Customer\":\"Is possible to do that ?\"},{\"Customer\":\"Can we have a demo?\"},{\"Customer\":\"So fast as you can do\"},{\"Customer\":\"We want to integrate with our website, salesforce, an our App\"},{\"Customer\":\"company-site.com\"},{\"Customer\":\"Investment\"},{\"Steve\":\"Ok great, thanks for sharing that information. yes a Demo and chat may be the best way to proceed.\"},{\"Customer\":\"Maybe we can have a video call?\"},{\"Steve\":\"Of course. Can you share your availability for Thursday or Friday this week?\"},{\"Customer\":\"Sure, hold on please\"},{\"Steve\":\"No problem\"},{\"Customer\":\"May be 2:00 pm Mexico Time?\"},{\"Customer\":\"Thursday?\"},{\"Steve\":\"Yes that\'s fine for us. Can you let me know your email address and I will send you a Zoom invite\"},{\"Customer\":\"<email>\"},{\"Steve\":\"Ok, thanks you Emilio. Nice to meet you. I will send an invite now. I will show you to the Talkative Chat, Video and Cobrowse working in Salesforce and you can ask any questions you may have\"},{\"Customer\":\"Perfect!!!\"},{\"Customer\":\"In advanced it is possible to get a Qoute for 30 advisors?\"},{\"Steve\":\"You should have an invite now\"},{\"Customer\":\"Thank you!!\"},{\"Customer\":\"I don\'t have it yet, but I gess is on the way\"},{\"Steve\":\"Hi Emilio, for Budget reasons we would quote apprx $27,000 USD for one year. But we can discuss your requirements in more detail\"},{\"Steve\":\"Should be on it\'s way now\"},{\"Customer\":\"Richt, I understand, works for me!!! thank you!!!\"},{\"Customer\":\"I appreciate your help!!!\"},{\"Steve\":\"No trouble at all. Look forward to speaking to you Thursday. Saty safe\"},{\"Customer\":\"Same to you!!!\"},{\"Customer\":\"Have a nice day!!!\"},{\"Steve\":\"Thanks, you too\"},{\"Steve\":\"I will end the conversation now but email me back with any other queries\"},{\"Steve\":\"<email>\"},{\"Customer\":\"Sure\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (690453, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Hi I\'m trying to reset my password for my trial account and it\'s not sending an email\"},{\"Talkative\":\"Sorry, I didn\'t quite catch that. Please rephrase your question, or click below to speak to the team:\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Customer\":\"My email is <email>\"},{\"Steve\":\"Hello! Welcome to the Talkative website. My name is Steve. How can I help you?\"},{\"Steve\":\"Hi, do you need help with your password still?\"},{\"Steve\":\"Often our email get\'s blocked by Spam and Junk filters. Are you able to check for it there?\"},{\"Steve\":\"Our email servers are currently working so you should be able to use this link https://us.engage.app/password/reset\"},{\"Customer\":\"hmp\"},{\"Customer\":\"ok\"},{\"Customer\":\"I\'ll give it an other try\"},{\"Customer\":\"yea I checked my spam folder (it\'s managed by google)\"},{\"Steve\":\"ok\"},{\"Customer\":\"hmp it worked that time\"},{\"Customer\":\"**** I see what happen\\nI used this link\\nhttps://engage.talkative.uk/login/sso instead of the link you provided - why is there 2 different urls?\"},{\"Steve\":\"Ah, the SSO link is to login using Single Sign on. That would require some work to setup so we can authenticate with Azure or Active Directory\"},{\"Customer\":\"ok\"},{\"Customer\":\"yea just very confusing to new users **\"},{\"Customer\":\"because I don\'t see a link to that page from the talkative site\"},{\"Customer\":\"o well\"},{\"Customer\":\"Thank you for your time!\"},{\"Customer\":\"have a great day!\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (691563, 'Felix', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Felix\":\"Hello! Welcome to the Talkative website. My name is Felix. How can I help you?\"},{\"Customer\":\"Hi I have signed up for a free trial, will the details come through via email on how to get started?\"},{\"Felix\":\"Let me check for you. What\'s your email please?\"},{\"Customer\":\"<email>\"},{\"Felix\":\"Hi Brendon, let me send you out a welcome email. I can see you have requested a trial\"},{\"Customer\":\"Yes.\"},{\"Customer\":\"I did have another question. If you just want chat or wanted chat and video call do you need to place different code in your site, or do you place the same code, but change something in a backend control panel to chat what the visitor has access too?\"},{\"Customer\":\"Sorry should have ready to change what the visitor has access too?\"},{\"Felix\":\"for now it will be different script config on those different pages\"},{\"Felix\":\"are you using google tag manager? or direct code injection?\"},{\"Customer\":\"I have limited access to the code on the site. but it will be a direct code injection. I have created a virtual experience and there is only one html page that everything runs in. Hence requesting a trial, so I can test out what can be done with what I have access too. The software I use creates the html for me, so I will have to break the code.\"},{\"Felix\":\"got it\"},{\"Felix\":\"we are releasing (later in april) a brand new version of our scripts that gives you a lot more control over offering different experiences on different pages, which sounds like it would make this process easier for you\"},{\"Customer\":\"Are you able to direct me to a tutorial or instruction of the script and where it sits on the webpage?\"},{\"Customer\":\"Im guessing it either sits in the header of footer???\"},{\"Felix\":\"hi, you should now have access\"},{\"Felix\":\"you can generate the script here: https://eu.engage.app/script-generator\"},{\"Felix\":\"and here is a guide: https://support.talkative.uk/technical/adding-scripts\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (730006, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"hi\"},{\"Talkative\":\"Hello! How can I help you?\"},{\"Customer\":\"can i get brochure talking about Mitel Contact Centre Integration\"},{\"Talkative\":\"Sorry, I didn\'t quite catch that. Please rephrase your question, or click below to speak to the team:\"},{\"Customer\":\"Mitel Contact Centre Integration brochure or presentation\"},{\"Talkative\":\"Sorry, I didn\'t quite catch that. Please rephrase your question, or click below to speak to the team:\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Steve\":\"Hi, you are through to Steve. Can see you are interested in Mitel integration. Are you a customer or Mitel partner?\"},{\"Customer\":\"a potential customer\"},{\"Steve\":\"Ok, do you use Mitel Contact Center Business?\"},{\"Customer\":\"yes\"},{\"Steve\":\"Ok this may be a good reference for you. https://www.youtube.com/watch?v=Px4aMnMHtBI\"},{\"Steve\":\"Do yuo have a website that you want to use Talkative on?\"},{\"Customer\":\"planning to come up with one\"},{\"Customer\":\"also want a document or brochure that i can present to my line manager please\"},{\"Steve\":\"Ok I can send you one via email. Can you share your address with me?\"},{\"Customer\":\"<email>\"},{\"Customer\":\"will really appreciate\"},{\"Steve\":\"No problem, that;s on the way. You have my contact details there. Get in touch if you would like to arrange a demonstration or further discussion\"},{\"Steve\":\"FYI, as well as Mitel integration you can also use Talkative within Salesforce and we can also deploy as a Standlaone application, all you need is a web browser.\"},{\"Customer\":\"ok noted\"},{\"Steve\":\"Do you have any other questions today?\"},{\"Customer\":\"just a moment\"},{\"Customer\":\"am just reading thro the doc\"},{\"Steve\":\"no problem\"},{\"Customer\":\"do you have a more detailed technical proposal\"},{\"Steve\":\"We would need to understand more about your environment. But you can review our help files at https://support.talkative.uk/\"},{\"Customer\":\"thanks Steve\"},{\"Customer\":\"let me go through shared info\"},{\"Customer\":\"much appreciated\"},{\"Customer\":\"if i need a demo will come back\"},{\"Steve\":\"ok, thanks, have a good day\"},{\"Customer\":\"have a good day too\"},{\"Customer\":\"bye for now\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (733264, 'Felix', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Customer\":\"thanks\"},{\"Felix\":\"Hello! Welcome to the Talkative website. My name is Felix. How can I help you?\"},{\"Customer\":\"Hi I just wanted to ask about your service and wanted to know if you offer a portal/dashboard for users to connect to agents via video chat with native sdks\"},{\"Customer\":\"iOS & android\"},{\"Felix\":\"Yes, do you mean integrating into an existing mobile app?\"},{\"Customer\":\"Yes\"},{\"Felix\":\"yes, i can send over some docs if you like, it\'s quite new so not on our support portal yet\"},{\"Customer\":\"SureSo my question is possible\"},{\"Felix\":\"Yes, to confirm our video/chat can be embedded into an existing app e.g. mobile banking app\"},{\"Customer\":\"Right and users can connect to agents that are online and you\'re sdk will tell us agents that are available etc?\"},{\"Felix\":\"Yes although it works on a queue basis\"},{\"Customer\":\"Ok but if there are no agents available it would tell us so we can handle this in our code?\"},{\"Felix\":\"So agents are part of a queue, if all agents are offline and/or you are outside of business hours then our online endpoint would tell you that the queue is offline\"},{\"Felix\":\"yea\"},{\"Customer\":\"Sweet\"},{\"Customer\":\"So I have a few more reqs do you following the following\"},{\"Customer\":\"* \\t\\tData centre location – must be in the UK\\n* \\t\\tTLS protocol\\n* \\t\\tDatabase encryption – AES 256bit required\"},{\"Felix\":\"yes to all the above, tls 1.2, we use AWS london DCs\"},{\"Customer\":\"Perfect, so your dashboard where you can see people to connect too is this a separate portal and can you invite multiple agents\"},{\"Felix\":\"not 100% sure what you mean, you can have multiple agents on the platform but it is mainly a 1-1 contact platform, rather than multiparty conference\"},{\"Customer\":\"I mean that lets say I have two agents, they can both access the portal and manage/connect to users who are looking to video chat\"},{\"Felix\":\"yes\"},{\"Felix\":\"although it\'s inbound rather than outbound. so the agents are waiting on app/website users to initiate the chat\"},{\"Customer\":\"Yh but agents can see who is waiting and connect to them via the dashboard/portal?\"},{\"Felix\":\"unfortunately not. that is roadmap for web but im not sure if that would be possible with the app. the system is currently built with the idea of a contact centre type flow where the system allocates interctions to agents\"},{\"Customer\":\"I\'m a bit confused. Do you have a contact number so we could possibly discuss this on call maybe?\"},{\"Felix\":\"sure i am on <phoneNumber>\"},{\"Felix\":\"can we do the call at midday if that works? or 1145?\"},{\"Customer\":\"So you\'re not free now?\"},{\"Felix\":\"sure i can do now\"},{\"Customer\":\"perfect\"},{\"Customer\":\"<email>\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (737476, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Customer\":\"ok\"},{\"Steve\":\"Hi you are through to Steve - How can I help you?\"},{\"Customer\":\"I am good you tell. I need some assitane about this talkative tool\"},{\"Customer\":\"i need to have demo right now so i can know how this things work on website\"},{\"Customer\":\"As we are startup agency and we want a chat system that we can communicate with customer in different language\"},{\"Steve\":\"Ok, yes Talkative can help with that. First of can you tell me your name?\"},{\"Customer\":\"waleed\"},{\"Steve\":\"Hi Waleed, thanks, and can you link me to your website or share what the website is for?\"},{\"Customer\":\"<url>\"},{\"Customer\":\"here is the link\"},{\"Customer\":\"its a digital agency\"},{\"Customer\":\"we want to have connection with customers but they need to know that we are talking in same language but we are in english and they will recieve it in german\"},{\"Customer\":\"and when they sent message in german we will receive it in english\"},{\"Customer\":\"this what i wanted\"},{\"Steve\":\"Sure, yes we use Google Translate v3 API for our translate which support 110 languages now\"},{\"Customer\":\"can you show me the demo that how this thing work\"},{\"Steve\":\"I can Walleed, would you like to join a Zoom session now so I can show you?\"},{\"Customer\":\"sure sure\"},{\"Steve\":\"give me 2 minutes to setup. Can you share your email address so I can send you the link. I\'ll need to drop off this chat to log into the demo system\"},{\"Customer\":\"<email>\"},{\"Steve\":\"Ok, I\'ll send the link over in 2 minutes. Saying goodbye here for now\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (738220, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Steve\":\"Hi this is Steve. How can I help you?\"},{\"Customer\":\"Hi, do you have a check list comparison with the Mitel standard chat function?  Like to see what additional functions we would get.\"},{\"Steve\":\"Sure, can you let me know your name and email?\"},{\"Steve\":\"I can send you a document\"},{\"Customer\":\"Mat Test <email>\"},{\"Customer\":\"The comparison with Live Chat is helpful, but would be good to see comparison with Mitel - which is what we currently have.  Thanks\"},{\"Steve\":\"Hi Mat, this is Steve. I ran the demo a few months ago with you. I will send over our document. It\'s primarily aimed at Partners but should give you a good guide. Anything further let me know\"},{\"Customer\":\"Ah thanks Steve - couldn\'t tell from the photo whether that was you!  Sorry and many thanks.\"},{\"Steve\":\"no worries. Look for an email in 10 minutes\"},{\"Steve\":\"Have a great day\"},{\"Customer\":\"Thanks, you to\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (759028, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Speak to the team\"},{\"Talkative\":\"Sure, will transfer you now!\"},{\"Customer\":\"Hi, I\'m trialling Talkative for a possible Mitel integration and I\'ve selected web chat in the script generator, but it\'s not showing after deploying to our test website using google tag manager\"},{\"Steve\":\"Ok, happy to help, This is Steve, can you let me know your name and email?\"},{\"Customer\":\"Hi Steve, My name\'s Simon Smith and email is <email>\"},{\"Steve\":\"Hi Simon, I just emailed you about 10 minutes ago. Ok, so have you created a new queue or are you using the Default Queue?\"},{\"Customer\":\"Just the default queue\"},{\"Steve\":\"When you view the Queue in the Admin interface there is a Column called \\\"Online\\\" can you check that has a Green tick against that queue?\"},{\"Steve\":\"Should be on this link https://eu.engage.app/queues\"},{\"Customer\":\"Ah okay, I wasn\'t online. That\'s sorted it. Thank you!\"},{\"Steve\":\"Yes, the widget will only \\\"Fire\\\" if it;s within business hours and there is an Agent signed in. You can also set a queue cap on the Queue so that you can queue more than the total number of available agents. If that makes sense\"},{\"Steve\":\"You are welcome. Would be good to talk once you have had a play so email me directly at <email> and we can discuss your requirement. There is more customisation and options available than the base trial allows.\"},{\"Customer\":\"That\'s great, cheers! While I have you, to what level can we edit the chat window widget please? Is it possible to use CSS overrides and replace buttons?\"},{\"Steve\":\"Indeed, as part of our service we work with you to build a widget that matches your own brand guidelines and design. However you can also edit the solution yourself. There\'s some detail on our help page here https://support.talkative.uk/technical/ui-design-options\"},{\"Steve\":\"We are also developing a Self Service widget builder which will be available in a couple of months. That will allow you quite a high level of customisation without any CSS / HTML knowledge\"},{\"Customer\":\"Thanks Steve. That\'s perfect\"},{\"Steve\":\"No problem. I\'ll give you a week or so to play and let\'s catch up next week if you aren\'t away for Easter? Or if you want to talk sooner please email or call, my details are in the email I sent over.\"},{\"Customer\":\"Next week would be great. Appreciate your help\"},{\"Steve\":\"Have a good week\"},{\"Customer\":\"Cheers, enjoy your Easter weekend!\"},{\"Steve\":\"Thanks\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (781287, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Hi!\"},{\"Talkative\":\"Hello! How can I help you?\"},{\"Customer\":\"Want to speak to a human\"},{\"Talkative\":\"Sure, we will transfer you now! We\'d also appreciate it if you can share your email address\"},{\"Customer\":\"<email>\"},{\"Steve\":\"Hi Esteban, apologies for th delay getting to you.\"},{\"Steve\":\"My name is Steve. How can I help you?\"},{\"Customer\":\"Hi Steve, no worries\"},{\"Customer\":\"I have several questions about talkative\"},{\"Steve\":\"ok, fire away\"},{\"Customer\":\"is it possible to just have the call function on the widget? not allowing customers to chat but only call?\"},{\"Steve\":\"Yes indeed, our customer <url> does the same. Follow that link and you should see a telephone in the lower left side\"},{\"Steve\":\"And you can offer Video as an option, As our customer <url> does. (Although they are offline right now due to timezones)\"},{\"Customer\":\"nice!\"},{\"Customer\":\"can we add fields to the form before the client can connect with us?\"},{\"Steve\":\"You can yes, if you want to capture some information beforehand, like email, name, etc\"},{\"Customer\":\"amazing!\"},{\"Customer\":\"do you have Salesforce integration?\"},{\"Steve\":\"We do yes, follow the link I\'m sending you now\"},{\"Steve\":\"There\'s further info on App Exchange https://appexchange.salesforce.com/appxListingDetail?listingId=a0N3A00000FtWP2UAN\"},{\"Steve\":\"We are a lightening application for Sales and Service clouds\"},{\"Customer\":\"thanks!\"},{\"Customer\":\"I have a couple more other questions\"},{\"Customer\":\"how customizable is the widget? and is it available for all countries?\"},{\"Steve\":\"Yes we can fully customise the widget, Either we deploy using Code snippet we provide or we can use GTM which you have on your website\"},{\"Steve\":\"The customer widget can be in any or multiple languages\"},{\"Steve\":\"The Talkative admin and agent interface is onl available in English today though\"},{\"Customer\":\"great news!\"},{\"Steve\":\"For chat we do have translation built in also\"},{\"Customer\":\"last question before I keep on doing my research on your oage\"},{\"Customer\":\"do the calls get directed into Salesforce to the agents or calls are made through your own interface/tool?\"},{\"Steve\":\"For Salesforce we provide a softphone within Salesforce that we deliver the calls to\"},{\"Customer\":\"great!\"},{\"Customer\":\"would it be possible for me to see how that looks like?\"},{\"Steve\":\"Yes, I can arrange a demonstration for you\"},{\"Customer\":\"that would be great!\"},{\"Customer\":\"I have scheduled one before speaking with you for today at 14.30\"},{\"Customer\":\"not sure if that would work\"},{\"Steve\":\"Ah yes, perfect\"},{\"Steve\":\"We can cover it at 14:30\"},{\"Customer\":\"with Felix Winstone\"},{\"Customer\":\"would you be able to speak with him or join us?\"},{\"Steve\":\"Speaking to him now to cover requirements\"},{\"Customer\":\"awesome, thanks!\"},{\"Steve\":\"ok, Speak more at 2:30\"},{\"Customer\":\"sounds good, thanks for everything!\"},{\"Steve\":\"thanks\"}]');
+INSERT INTO chat_record (chat_record_id, agent_name, transcript) VALUES (804161, 'Steve', '[{\"Talkative\":\"Hi, welcome to Talkative! How can we help? Please ask us anything, or click the buttons below. Let us know if you\'d prefer to speak to a human!\"},{\"Customer\":\"Support\"},{\"Talkative\":\"If you\'re have a technical issue, please email support@talkative.uk or transfer to the team. Please note we have a support portal here for FAQs: https://support.talkative.uk/\"},{\"Customer\":\"do you provide multi agent facility?\"},{\"Talkative\":\"Sure, we will transfer you now! We\'d also appreciate it if you can share your email address\"},{\"Customer\":\"<email>\"},{\"Steve\":\"Hi, you are connected with Steve, how can I help you?\"},{\"Customer\":\"do you provide multi agent facility?\"},{\"Steve\":\"Do you mean having Queues with multiple agents available to handle the website interactions? If so yes we do\"},{\"Customer\":\"yes\"},{\"Customer\":\"is that support in video chat ?\"},{\"Steve\":\"There is no limitations on number of users. How many agents are you expecting to handle customer chats?\"},{\"Steve\":\"Yes we can provide video chat,\"},{\"Customer\":\"may 5 - 10 agents\"},{\"Customer\":\"is that video chat with multi agent?\"},{\"Steve\":\"Ah, no not at this stage, video is 1-1\"},{\"Steve\":\"Agent to customer\"},{\"Steve\":\"Agents can transfer the call to another agent if required. But only 1 party is connected at a time\"},{\"Steve\":\"Is that a requirement or a \\\"nice to have?\\\"\"},{\"Customer\":\"we have requirement\"},{\"Customer\":\"if our one agent is busy with one customer, then we need to transfer next video query to another agent by automatically. is that possible?\"},{\"Steve\":\"yes, that is how the system works. If you have multiple agents available in a queue, the interaction is delivered to the longest idle agent. If there are no agents available you have two options, you can stop the widget dsiplaying on your website, or you can capture the customers lead data and we can send that to you as an email\"},{\"Steve\":\"If you would like to try the system out we can set you up a 14 day trial. There is no commitment at the end and you do not need to provide any billing information\"},{\"Customer\":\"yes, we interested in trial\"},{\"Customer\":\"how cal i rdeem that\"},{\"Steve\":\"ok no problem. All I need is your Name and email address. You will use that to sign into our system. One you get the invite email and can sign in we will have a quick Zoom call where I can set you up and show you how to configure a demo for yourself.\"},{\"Customer\":\"ok\"},{\"Customer\":\"name is Ajay\"},{\"Customer\":\"<email>\"},{\"Steve\":\"Ok, nice to meet you Ajay. You will get an email from us within an hour. Check you junk / spam folder if you haven\'t seen it by then. Once you have your account setup we can jump on Zoom and set you up. Do you have time today to do that? or would you like to schedule the meeting for later this week?\"},{\"Customer\":\"can i change the mail id?\"},{\"Customer\":\"i lost the access with that id\"},{\"Steve\":\"sure, let me know an alternative email address\"},{\"Customer\":\"<email>\"},{\"Steve\":\"it may take a few minutes\"},{\"Steve\":\"should be on the way now\"},{\"Customer\":\"let me check\"},{\"Customer\":\"CAN U PLEASE REDIRECT IT TO <email>\"},{\"Steve\":\"Ok\"},{\"Customer\":\"are u done?\"},{\"Steve\":\"Is this an email address that you have access to? You have to be able to click the link in the email so it need sot be a valid email that you can receive\"},{\"Steve\":\"Do you have a personal email address we can use?\"},{\"Customer\":\"yes i have access to <email>\"},{\"Customer\":\"<email>\"},{\"Steve\":\"ok, keep an eye on that email\"},{\"Customer\":\"ok\"},{\"Customer\":\"hi\"},{\"Customer\":\"now im in the dashboard\"},{\"Customer\":\"could you please give me a user support\"},{\"Steve\":\"Of course, please join me on Zoom here\\nJoin Zoom Meeting\\nhttps://us02web.zoom.us/j/6812471204\"}]');
